		"@sveltejs/kit": "^1.27.4",
		"@types/d3": "^7.4.3",
		"autoprefixer": "^10.4.14",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-svelte": "^2.30.0",
		"flowbite": "^2.1.1",
		"flowbite-svelte": "^0.44.19",
		"postcss": "^8.4.24",
		"postcss-load-config": "^4.0.1",
		"prettier": "^3.0.0",
		"prettier-plugin-svelte": "^3.0.0",
		"svelte": "^4.0.5",
		"svelte-check": "^3.6.0",
		"tailwindcss": "^3.3.2",
